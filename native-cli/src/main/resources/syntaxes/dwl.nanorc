syntax "DWL" "\.dwl$"
header "^\{$"
# You can't add a comment to DWL.
comment "//"
color cyan   "\<[-]?[1-9][0-9]*([Ee][+-]?[0-9]+)?\>"  "\<[-]?[0](\.[0-9]+)?\>"
color magenta  "\<null\>"
color magenta "\<(true|false)\>"
color blue "[a-zA-Z0-9]+\s*(@|:)"
color white ":|\{|\}|\[|\]|,"
color green "@|\(|\)"
color green ""(\\.|[^"])*"|'(\\.|[^'])*'"
